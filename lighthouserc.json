{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "url": ["/index.html"],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.9 }],
        "categories:accessibility": ["error", { "minScore": 0.95 }]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouse"
    }
  }
}
